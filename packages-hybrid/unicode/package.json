{
    "name": "@nlib/unicode",
    "publishConfig": {
        "access": "public"
    },
    "version": "3.3.1",
    "main": "lib/index.js",
    "files": [
        "lib"
    ],
    "scripts": {
        "clear-cache": "rimraf build/__cache",
        "cleanup": "rimraf lib",
        "build:named": "ts-node build/named.ts",
        "tsc": "tsc",
        "build": "npm-run-all --serial build:named cleanup tsc",
        "test": "ava --verbose lib/*.test.js",
        "lint": "eslint --ext .ts src build"
    },
    "dependencies": {
        "@nlib/global": "^3.2.0"
    },
    "devDependencies": {
        "@nlib/afs": "^3.2.0",
        "@nlib/infra": "^3.3.0",
        "@nlib/node-stream": "^3.3.1",
        "@nlib/node-util": "^3.3.1"
    },
    "eslintConfig": {
        "overrides": [
            {
                "files": "build/*.ts",
                "env": {
                    "node": true
                },
                "rules": {
                    "no-console": "off"
                }
            }
        ]
    }
}
