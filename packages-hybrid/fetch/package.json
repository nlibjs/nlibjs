        "lint": "eslint --ext .ts src"
    },
    "dependencies": {
        "@nlib/global": "^3.2.0",
        "@nlib/infra": "^3.2.0",
        "@nlib/real-number": "^3.2.0"
    },
    "eslintConfig": {
        "overrides": [
            {
                "files": "src/*.ts",
                "env": {
                    "node": true
                },
                "rules": {
                    "no-loop-func": "off"
                }
            }
        ]
    }
